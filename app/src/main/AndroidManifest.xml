<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daute.crudfinal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Autorizacion">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.slideshow.EditarProducto" />
        <activity android:name=".ui.slideshow.DetalleProducto" />
        <activity android:name=".ui.slideshow.GuardarProductos" />
        <activity android:name=".ui.gallery.EditarUsuario" />
        <activity android:name=".ui.gallery.DetalleUsuario" />
        <activity android:name=".ui.gallery.GuardarUsuario" />
        <activity android:name=".ui.home.EditarCategoria" />
        <activity android:name=".ui.home.DetalleCategoria" />
        <activity android:name=".ui.home.GuardarCategoria" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>